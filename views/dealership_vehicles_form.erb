<h1>Add a vehicle <%= @dealership.name() %> </h1>

<form action="/vehicles" method="post">
  <input type="hidden" name="dealership_id" id="dealership_id" value="<%= @dealership.id() %>">

  <label for="make">Vehicle's make:</label>
  <input type="text" name="make" id="make">

  <label for="model">Vehicle's model:</label>
  <input type="text" name="model" id="model">

  <label for="year">Vehicle's year:</label>
  <input type="text" name="year" id="year">

  <button type="submit">Add Vehicle</button>
</form>
